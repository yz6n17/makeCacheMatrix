# makeCacheMatrix

makeCacheMatrix <- function(x = numeric()) {
        m <- NULL
        set <- function(z) {
                x <<- z
                m <<- NULL
        }
        get <- function() x
        setsolve <- function(solve) so <<- solve
        getsolve <- function() so
        list(set = set, get = get,
             setsolve = setsolve,
             getsolve = getsolve)
} 

# cacheSolve
cacheSolve <- function(x) {
        so <- x$getsolve()
        if(!is.null(so)) {
                message("getting cached data")
                return(so)
        }
        data <- x$get()
        so <- solve(data, ...)
        x$setsolve(so)
        so
}
